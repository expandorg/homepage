{
  "private": true,
  "name": "requester-landing",
  "version": "0.0.1",
  "license": "ISC",
  "author": "Expand.org",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/template.html --service-worker false",
    "serve": "preact build --template src/template.html --service-worker false && preact serve",
    "dev": "preact watch --port 3003 --template src/template.html",
    "lint": "eslint src"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-render-to-string": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-synacor": "^1.1.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "flow-bin": "^0.104.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0",
    "prettier": "^1.18.2"
  }
}
